object Col_14: TbeAttr
  Caption = 'Verf'#252'gbar'
  Width = 10
  DisplayWidth = 10
  Alignment = taCenter
  Freezed = False
  ColResultBlock = 
    '| aAvail:N:=0, cResult, cAnummer:C:='#39#39',nX, aApnummer, nPos, nBes' +
    'tand:N:=0 |'#13#10#13#10'// Die Funktionalit'#228't basiert darauf, dass dieser' +
    ' Codeblock immer wieder'#13#10'// ausgef'#252'hrt, aber nicht neu initialis' +
    'iert wird. D.h. die Variablen haben'#13#10'// beim erneuten Ausf'#252'hren ' +
    'noch den Wert der letzten Ausf'#252'hrung haben'#13#10'// -> in aApnummer w' +
    'erden die bereits errechneten Werte als'#13#10'//    Array gespeichert' +
    ' und evtl. wiederverwendet'#13#10'// -> Performance-Steigerung, denn A' +
    'ufPosVerfueg() wird nicht'#13#10'//    unn'#246'tig oft aufgerufen'#13#10#13#10'cResu' +
    'lt := "",'#13#10#13#10'if AllTrim(ARTNR) <> '#39'@@@@@'#39'                  // ke' +
    'ine Textpos'#13#10'   and not empty(ARTNR)                       // ke' +
    'in leere DS'#13#10'   and (bmenge-vmenge-lmenge) > 0             // R'#252 +
    'ckstand > 0'#13#10'   and (auf_lag='#39'J'#39' or Dab010:mindbest=-2)    // la' +
    'gerwirksam oder -2'#13#10'then'#13#10'   if SubStr(apnummer,1,6) <> cAnummer' +
    ' then   // bei ge'#228'ndertem Auftrag'#13#10'      aApnummer := {},       ' +
    '                        // Array leeren'#13#10'      cAnummer  := SubS' +
    'tr(apnummer,1,6),'#13#10'   endif,'#13#10'   nPos:=0,'#13#10'   for nX:=1 to len(a' +
    'Apnummer) do                    // ist die aktuelle Auftragspos.' +
    #13#10'      if aApnummer[nX,1] = APNUMMER then      // bereits im Ar' +
    'ray enthalten?'#13#10'         nPos:=nX,                              ' +
    '     // -> dann Position merken'#13#10'         break,'#13#10'      endif,'#13#10 +
    '   next,'#13#10'   // Lagerbestand merken, denn dieser k'#246'nnte sich ge'#228 +
    'ndert haben'#13#10'   nBestand := AsVariant("LAGER" + ltrim( str(lager' +
    ',2) ), Dab010),'#13#10'   // Pr'#252'fen, ob sich ein f'#252'r die Verf'#252'gbarkeit' +
    ' relevanter Parameter ge'#228'ndert hat'#13#10'   if empty(nPos)'#13#10'      or ' +
    'aApnummer[nPos,3] <> bmenge-lmenge-vmenge // anderer R'#252'ckstand'#13#10 +
    '      or aApnummer[nPos,4] <> nBestand             // anderer La' +
    'gerbestand'#13#10'      or aApnummer[nPos,5] <> dab010:ruecks_pr     /' +
    '/ andere Produktions-Reservierungen'#13#10'      or aApnummer[nPos,6] ' +
    '<> dab010:rueckstand    // andere Kunden-Reservierungen'#13#10'      o' +
    'r aApnummer[nPos,7] <> priori               // andere Priorit'#228't'#13 +
    #10'      or aApnummer[nPos,8] <> amenge               // andere '#39'b' +
    'eschaffte Menge'#39' (relevant bei -2 Artikel)'#13#10'      or aApnummer[n' +
    'Pos,9] <> rmenge               // andere Reservierte Menge'#13#10'   t' +
    'hen'#13#10'      aAvail  := AufPosVerfueg(Dab055, Dab010),    // Verf'#252 +
    'gbarkeit pr'#252'fen'#13#10'      cResult := '#39'Nein'#39','#13#10'      if aAvail = 0 t' +
    'hen'#13#10'         cResult := '#39'Ja'#39','#13#10'      endif,'#13#10'      if aAvail = ' +
    '1 then'#13#10'         cResult := '#39'Teilw.'#39','#13#10'      endif,'#13#10'      if aA' +
    'vail = 2 then'#13#10'         cResult := '#39'???'#39','#13#10'      endif,'#13#10#13#10'     ' +
    ' // Da eine '#196'nderung der Reservierungsmenge Auswirkungen auf and' +
    'ere'#13#10'      // Positionen des selben Auftrags haben kann (wenn Ar' +
    'tikel gleich), muss die'#13#10'      // gesamte Verf'#252'gbarkeit neu gepr' +
    #252'ft werden -> Array leeren'#13#10'      if not empty(nPos)'#13#10'         a' +
    'nd aApnummer[nPos,9] <> RMENGE then'#13#10'         aApnummer := {},'#13#10 +
    '         nPos      := 0,'#13#10'      endif,'#13#10#13#10'      if empty(nPos) t' +
    'hen                          // wenn noch kein Array-Eintrag exi' +
    'stiert'#13#10'         AAdd(aApnummer, {apnummer, cResult, bmenge-lmen' +
    'ge-vmenge, nBestand,'#13#10'                          dab010:ruecks_pr' +
    ', dab010:rueckstand, priori, amenge, rmenge}),'#13#10'         nPos :=' +
    ' len(aApnummer),'#13#10'      else                                    ' +
    '     // ansonsten existierenden '#252'berschreiben'#13#10'         aApnumme' +
    'r[nPos,2] := cResult,'#13#10'         aApnummer[nPos,3] := bmenge-lmen' +
    'ge-vmenge,'#13#10'         aApnummer[nPos,4] := nBestand,'#13#10'         aA' +
    'pnummer[nPos,5] := dab010:ruecks_pr,'#13#10'         aApnummer[nPos,6]' +
    ' := dab010:rueckstand,'#13#10'         aApnummer[nPos,7] := priori,'#13#10' ' +
    '        aApnummer[nPos,8] := amenge,'#13#10'         aApnummer[nPos,9]' +
    ' := rmenge,'#13#10'      endif,'#13#10'   else'#13#10'      cResult := Aapnummer[n' +
    'Pos,2],'#13#10'   endif,'#13#10'endif,'#13#10#13#10'cResult'
