  FuncBlock = 
    'function GetAuftragsPositionsStatusText(nStatus)'#13#10'   case nStatu' +
    's'#13#10'      of 10 ::  Result := '#39'Im Versand'#39','#13#10'      of 20 ::  Resu' +
    'lt := '#39#39', // '#39'Offen'#39','#13#10'      of 30 ::  Result := '#39'Teilw. geliefe' +
    'rt'#39#13#10'      of 40 ::  Result := '#39'Geliefert'#39','#13#10'      of 50 ::  Res' +
    'ult := '#39'Teilw. fakturiert'#39','#13#10'      of 60 ::  Result := '#39'Fakturie' +
