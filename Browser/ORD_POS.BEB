    '| nStatus |'#13#10#13#10'// Berechnet den Status der Position f'#252'r Auftr'#228'ge' +
    ' (nicht Angebote)'#13#10'function GetAuftragsPositionsStatus(oDab055)'#13 +
    #10'| nFakturaRueckstand, nLieferRueckstand |'#13#10#13#10'   Result := 0,'#13#10' ' +
    '  if oDab055:BMENGE <> 0 then'#13#10#13#10'      nLieferRueckstand  := oDa' +
    'b055:BMENGE - oDab055:VMENGE - oDab055:LMENGE,'#13#10'      nFakturaRu' +
    'eckstand := oDab055:VMENGE + oDab055:LMENGE - oDab055:FMENGE,'#13#10#13 +
    #10'      // im Versand'#13#10'      if oDab055:VMENGE <> 0 then'#13#10'       ' +
    '  Result := 10,'#13#10'      else // => keine Versand-Freigabe'#13#10'      ' +
    '   if nLieferRueckstand <> 0 then'#13#10'            if oDab055:LMENGE' +
    ' = 0 then'#13#10'               Result := 20,                      // ' +
    'Offen - noch gar nichts geliefert'#13#10'            else'#13#10'           ' +
    '    Result := 30,                      // teilweise geliefert'#13#10' ' +
    '           endif,'#13#10'         else // => kein Liefer-R'#252'ckstand'#13#10'  ' +
    '          if nFakturaRueckstand <> 0 then'#13#10'               if oDa' +
    'b055:FMENGE = 0 then'#13#10'                  Result := 40,           ' +
    '        // noch gar nichts fakturiert aber alles geliefert'#13#10'    ' +
    '           else'#13#10'                  Result := 50,                ' +
    '   // teilweise fakturiert'#13#10'               endif,'#13#10'            e' +
    'lse // => kein Faktura-R'#252'ckstand'#13#10'               if oDab055:FMEN' +
    'GE = oDab055:BMENGE then'#13#10'                  Result := 60,       ' +
    '            // vollst'#228'ndig fakturiert'#13#10'               else'#13#10'    ' +
    '              Result := 20,'#13#10'               endif,'#13#10'            ' +
    'endif,'#13#10'         endif,'#13#10'      endif,'#13#10#13#10'   endif,'#13#10'end,'#13#10#13#10'func' +
    'tion GetAuftragsPositionsStatusText(nStatus)'#13#10'   case nStatus'#13#10' ' +
    '     of 10 ::  Result := '#39'Im Versand'#39','#13#10'      of 20 ::  Result :' +
